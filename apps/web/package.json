{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --force --port 5173",
    "build": "vite build",
    "build:production": "node scripts/build-production.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:feature-management": "playwright test featureManagement.e2e.test.ts",
    "test:e2e:feature-management:headed": "playwright test featureManagement.e2e.test.ts --headed",
    "test:e2e:feature-management:debug": "playwright test featureManagement.e2e.test.ts --debug",
    "test:e2e:communication": "playwright test communication-hub-*.spec.ts",
    "test:e2e:communication:ui": "playwright test communication-hub-*.spec.ts --ui",
    "test:e2e:communication:headed": "playwright test communication-hub-*.spec.ts --headed",
    "test:e2e:communication:debug": "playwright test communication-hub-*.spec.ts --debug",
    "test:e2e:load": "playwright test communication-hub-load-testing.spec.ts",
    "test:e2e:accessibility": "playwright test communication-hub-accessibility.spec.ts",
    "test:e2e:cross-browser": "playwright test communication-hub-cross-browser.spec.ts",
    "test:e2e:coverage": "node e2e/generate-coverage-report.cjs",
    "lighthouse": "lhci autorun",
    "lighthouse:collect": "lhci collect",
    "lighthouse:assert": "lhci assert",
    "lighthouse:upload": "lhci upload",
    "analyze": "ANALYZE=true npm run build",
    "bundle:analyze": "npm run analyze",
    "bundle:size": "node scripts/bundle-size-check.js",
    "bundle:measure": "node scripts/measure-bundle-size.cjs",
    "test:theme": "node scripts/run-theme-tests.js",
    "test:theme:performance": "vitest run src/__tests__/theme/ThemePerformance*.test.tsx --reporter=verbose",
    "test:theme:cls": "vitest run src/__tests__/theme/ThemeCLS*.test.tsx --reporter=verbose",
    "test:theme:visual": "vitest run src/__tests__/theme/ThemeVisual*.test.tsx --reporter=verbose",
    "test:theme:benchmark": "vitest run src/__tests__/theme/ThemePerformanceBenchmark*.test.tsx --reporter=verbose --timeout=60000",
    "test:performance": "node scripts/run-performance-tests.js",
    "test:performance:suite": "vitest run src/__tests__/performance/PerformanceTestSuite.test.tsx --reporter=verbose --timeout=60000",
    "test:performance:ci": "npm run test:performance -- --ci",
    "test:visual": "../scripts/run-visual-tests.sh",
    "test:visual:update": "../scripts/run-visual-tests.sh --update-snapshots",
    "test:visual:chromium": "../scripts/run-visual-tests.sh --browsers chromium",
    "test:load": "../scripts/run-load-tests.sh",
    "test:load:api": "../scripts/run-load-tests.sh api",
    "test:load:database": "../scripts/run-load-tests.sh database",
    "test:load:redis": "../scripts/run-load-tests.sh redis"
  },
