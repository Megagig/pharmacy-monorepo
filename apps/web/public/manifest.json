    "related_applications": [],
    "prefer_related_applications": false,
    "protocol_handlers": [
        {
            "protocol": "web+intervention",
            "url": "/clinical-interventions/view?id=%s"
        }
    ],
    "file_handlers": [
        {
            "action": "/clinical-interventions/import",
            "accept": {
                "text/csv": [
                    ".csv"
                ],
                "application/json": [
                    ".json"
                ]
            }
        }
    ],
    "share_target": {
        "action": "/clinical-interventions/share",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url",
            "files": [
                {
                    "name": "files",
                    "accept": [
                        "text/csv",
                        "application/json",
                        ".csv",
                        ".json"
                    ]
                }
            ]
        }
    }
